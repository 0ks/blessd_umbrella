<%= for attendant <- @attendants do %>
  <tr>
    <td>
      <input
      type="checkbox"
      class="js-attendant-is-present"
      data-id="<%= attendant.id %>"
      <%= if attendant.is_present, do: "checked" %> />
    </td>
    <td><%= attendant.person.name %></td>
    <td><%= attendant.person.email %></td>
  </tr>
<% end %>
